{
  "name": "koolhubz-infra",
  "version": "0.1.0",
  "bin": {
    "koolhubz-infra": "bin/koolhubz-infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint \"./lib/**/*.ts\" \"./bin/**/*.ts\"",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint \"./lib/**/*.ts\" \"./bin/**/*.ts\"  --fix",
    "lint:tests": "ESLINT_USE_FLAT_CONFIG=true eslint \"./test/**/*.ts\"",
    "format": "prettier --write \"lib/**/*.ts\" \"./bin/**/*.ts\"",
    "format:check": "prettier --check \"index.ts\" \"lib/**/*.ts\" \"*.json\"",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@types/uuid": "^10.0.0",
    "aws-cdk": "2.1026.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-appsync/utils": "^2.0.3",
    "@aws-sdk/client-cloudwatch": "^3.896.0",
    "@aws-sdk/client-dynamodb": "^3.893.0",
    "@aws-sdk/lib-dynamodb": "^3.893.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/ngeohash": "^0.6.8",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "aws-cdk-lib": "2.211.0",
    "constructs": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "geolib": "^3.3.4",
    "ngeohash": "^0.6.3",
    "prettier": "^3.6.2",
    "uuid": "^13.0.0"
  }
}
